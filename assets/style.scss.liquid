// Slider 5
.style5 {
  height: 100%;
  .slider-content {
    display: flex;
    align-items: center;
    flex-direction: column;
    justify-content: center;
    height: 100%;
    .row {
      width: 100%;
    }
    .slider-left-content {
      margin-right: 40px;
      text-align: right;
      height: 100%;
      display: flex;
      flex-direction: column;
      justify-content: center;
      @include media-query($max,$md){
        margin-right: 0;  
      }
      h1 {
        font-size: 50px;
        font-weight: 700;
        color: $black-color;
        font-family:$font-primary;
        @include media-query($max,$lg){
          font-size:40px;
        }
        @include media-query($max,$md){
          font-size:30px;
        }
        @include media-query($max,$sm){
          font-size: 20px;
        }        
      }
      h2 {
        font-size: 24px;
        font-weight: 300;
        font-family:$font-primary;
        color: $black-color;
        margin-bottom: 0;
        line-height: 40px;
        @include media-query($min,$lg){
          max-width: 336px;
          margin: 0 auto;
          margin-right: 0;
        }
        @include media-query($max,$md){
          font-size:18px;
          line-height: 28px;
        }
        @include media-query($max,$sm){
          font-size: 14px;
          line-height: 25px;
        }        
      }
      h3 {
        font-size: 28px;
        font-family:$font-primary;
        font-weight: 700;
        color: $black-color;
        line-height: 50px;
        @include media-query($max,$md){
          font-size:17px;
          line-height: normal;
        }
        @include media-query($max,$sm){
          font-size:14px;
        }
      }
      span.btn {
        padding: 16px 50px;
        background-color: #0467cd;
        text-align: center;
        color: #fff;
        text-transform: lowercase;
        font-size: 20px;
        margin-top: 40px;
        top: auto;
        max-width: inherit;
        margin-right: 0;
        width: auto;
        margin-left: auto;
        display: inline-block;
        @include media-query($max,$md){
          padding: 9px 22px;
        }
        @include media-query($max,$sm){
          padding: 9px 12px;
          font-size: 15px;
          margin-top:10px;
        }
      }
    }

    .slider-right-content {
      position: relative;
      margin-top: 40px;
      margin-bottom: 40px;
      .slide-active {
        position: relative;
        padding: 47% 0;
        width: 70%;
        margin: 0px auto;
        background-size: cover;
        z-index: 9;
        @include media-query($max,$md){
          padding: 67% 0;
          width: 100%;
        }
      }
      .slide-inactive-1{
        position: absolute;
        padding: 28% 0;
        width: 40%;
        left: 30px;
        bottom: -55px;
        background-size: cover;
        opacity: .7;
        z-index: 1;
        @include media-query($max,$md){
            padding: 32% 0;
            width: 62%;
            bottom: -42%;
          }
      }
      .slide-inactive-2{
        position: absolute;
        padding: 35% 0;
        width: 44%;
        right: 30px;
        top: -55px;
        background-size: cover;
        opacity: .7;
        z-index: 1;
        @include media-query($max,$md){
          width: 62%;
          top: -42%;
        }
      }
    }
  }
}

