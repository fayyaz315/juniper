}

.site-footer__newsletter {
  max-width: 320px;
  display:inline-block;

  @include media-query($min,$md) {
    max-width: none;
  }

  + div {
    margin-top: 25px;
  }
}

.site-footer__social-icons {
  margin-top: 15px;
}

.site-footer__copyright {
  margin-top: $section-spacing-small;
}

.social-icons__link {
  display: block;
  padding: 0 3px;

  &:first-child {
    margin-left: 0;
  }
}

.site-footer__copyright {
  text-align: center;
}

.site-footer__copyright-content {
  padding-right: 20px;

  .site-footer__copyright--right & {
    padding: 0 0 0 20px;

    &:first-child {
      padding-left: 0;
    }

    @include media-query($min,$md) {
      padding: 0 0 0 30px;
    }
  }
}

.site-footer__copyright-content--powered-by {
