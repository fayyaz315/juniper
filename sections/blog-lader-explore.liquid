<div class="explore-with explore-with-blogpage">
{% if section.settings.title != blank %}
<div class="section-header">
	<div class="container">
		<h2 data-aos="{{ section.settings.animation_title }}">{{ section.settings.title }}</h2>
	</div>
</div>
{% endif %}
<div class="explore-content container-fluid">
	<div class="row">
		<div class="col-lg-9 d-none d-xl-block"></div>
		<div class="col-sm-30 col-md-25 col-xl-20 pr-sm-0 text-center">
			<div data-aos="{{ section.settings.animation_image1 }}" class="explore-image explore-image1" style="background-image:url({{ section.settings.image1 | img_url: '1000x' }})"></div>
			<a data-aos="{{ section.settings.animation_button1 }}" href="{{ section.settings.link_1 }}" class="explore-left-btn">{{ section.settings.button_text_1 }}</a>
		</div>
		<div class="col-sm-30 col-md-35 col-xl-31 pr-xl-0">
			<div data-aos="{{ section.settings.animation_image2 }}" class="explore-image explore-image2" style="background-image:url({{ section.settings.image2 | img_url: '1000x' }})">
				<a data-aos="{{ section.settings.animation_button2 }}" href="{{ section.settings.link_2 }}">{{ section.settings.button_text_2 }}</a>
			</div>
		</div>
		<div class="col-lg-9 d-none d-xl-block">
		</div>
		<div class="col-sm-30 col-md-30 col-xl-26">
			<div data-aos="{{ section.settings.animation_image3 }}" class="explore-image explore-image3" style="background-image:url({{ section.settings.image3 | img_url: '1000x' }})">
			</div>
		</div>
		<div class="col-sm-30 col-md-30 col-xl-25">
			<div class="discover">
				<a data-aos="{{ section.settings.animation_button3 }}" href="{{ section.settings.link_3 }}">{{ section.settings.button_text_3 }}</a>
			</div>
		</div>
	</div>
</div>
</div>
{% if section.settings.foot_note != blank %}
<div class="bottom-content container-fluid" data-aos="{{ section.settings.animation_footer }}">
  <div class="row">
    <div class="col-lg-40 offset-lg-15 text-right">
      <div class="bottom-content-text">
      {{ section.settings.foot_note }}
      </div>

    </div>

  </div>
</div>
{% endif %}
